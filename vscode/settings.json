{
  // **************************************************************************
  // ** EXTENSIONS
  // **************************************************************************
  "code-eol.style": {
    "code-eol.crlfCharacter": "↵",
    "code-eol.newlineCharacter": "¬",
    "code-eol.returnCharacter": "⇠",
    "code-eol.validLineEnding": "LF",
    "color": "#5a5a5a",
    "opacity": 0.4
  },
  "gitlens.advanced.messages": {
    "suppressGitMissingWarning": true
  },
  "gitlens.codeLens.authors.enabled": false,
  "gitlens.codeLens.recentChange.enabled": false,
  "gitlens.currentLine.enabled": false,
  "prettier.arrowParens": "always",
  "prettier.bracketSpacing": true,
  "prettier.endOfLine": "lf",
  "prettier.insertPragma": false,
  "prettier.printWidth": 120,
  "prettier.semi": true,
  "prettier.singleQuote": false,
  "prettier.tabWidth": 2,
  "prettier.trailingComma": "es5",
  "prettier.useTabs": false,
  "python.formatting.provider": "black",
  "python.formatting.blackArgs": [],
  "python.linting.enabled": true,
  "shellformat.flag": "-i 2 -bn -ci -sr",
  // **************************************************************************
  // ** EDITOR
  // **************************************************************************
  "editor.bracketPairColorization.enabled": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": true,
    "source.organizeImports": true
  },
  "editor.guides.bracketPairs": "active",
  "editor.cursorSmoothCaretAnimation": true,
  "editor.cursorStyle": "line",
  "editor.cursorBlinking": "expand",
  "editor.cursorWidth": 1,
  "editor.detectIndentation": true,
  // "editor.fontFamily": "Source Code Pro",
  "editor.fontFamily": "JetBrainsMono NF",
  "editor.fontLigatures": true,
  "editor.fontSize": 13,
  "editor.fontWeight": "400",
  "editor.guides.indentation": true,
  "editor.inlayHints.enabled": "on",
  "editor.inlayHints.fontSize": 8,
  "editor.insertSpaces": true,
  "editor.lineHeight": 22,
  "editor.lineNumbers": "relative",
  "editor.matchBrackets": "never",
  "editor.minimap.enabled": false,
  "editor.mouseWheelZoom": true,
  "editor.quickSuggestions": {
    "other": "inline",
    "comments": "off",
    "strings": "inline",
  },
  "editor.renderWhitespace": "boundary",
  "editor.rulers": [
    80,
    120
  ],
  "editor.suggestSelection": "first",
  "editor.tabSize": 2,
  "editor.wordWrap": "wordWrapColumn",
  "editor.wordWrapColumn": 120,
  "editor.wrappingIndent": "same",
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  // **************************************************************************
  // ** LANGUAGES
  // **************************************************************************
  "emmet.includeLanguages": {
    "javascript": "html"
  },
  "html.format.wrapLineLength": 240,
  "html.format.preserveNewLines": true,
  "html.format.indentInnerHtml": false,
  "html.format.wrapAttributes": "auto",
  "html.format.enable": false,
  "markdown.preview.breaks": true,
  // **************************************************************************
  // ** CODELENS
  // **************************************************************************
  "html.autoClosingTags": false,
  "html.suggest.html5": true,
  "html.validate.scripts": true,
  "html.validate.styles": true,
  "javascript.inlayHints.functionLikeReturnTypes.enabled": true,
  "javascript.inlayHints.parameterNames.enabled": "all",
  "javascript.inlayHints.parameterTypes.enabled": true,
  "javascript.inlayHints.variableTypes.enabled": true,
  "javascript.referencesCodeLens.enabled": true,
  "javascript.updateImportsOnFileMove.enabled": "prompt",
  // **************************************************************************
  // ** FTDETECT
  // **************************************************************************
  "files.associations": {
    "Dockerfile-*": "dockerfile",
    "Dockerfile_*": "dockerfile"
  },
  "files.autoSave": "off",
  "files.autoSaveDelay": 10000,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/node_modules": true
  },
  // **************************************************************************
  // ** GIT
  // **************************************************************************
  "git.autofetch": true,
  "git.defaultCloneDirectory": "D:\\Git",
  "git.enableSmartCommit": true,
  "git.confirmSync": false,
  // **************************************************************************
  // ** FTPLUGIN
  // **************************************************************************
  "[c]": {
    "editor.defaultFormatter": "ms-vscode.cpptools"
  },
  "[css]": {
    "editor.suggest.insertMode": "replace"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.foldingStrategy": "indentation"
  },
  "[javascript]": {
    "editor.codeActionsOnSave": {
      "source.fixAll": false,
      "source.organizeImports": true
    },
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features",
    "editor.formatOnPaste": true,
    "editor.formatOnSave": false,
    "editor.formatOnType": false
  },
  "[jsonc]": {
    "editor.formatOnPaste": true,
    "editor.formatOnSave": false,
    "editor.formatOnType": false,
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[markdown]": {
    "editor.lineHeight": 24,
    "editor.overviewRulerBorder": false,
    "editor.quickSuggestions": {
      "comments": "off",
      "strings": "off",
      "other": "off"
    },
    "editor.wrappingIndent": "indent",
    "files.trimTrailingWhitespace": false
  },
  // **************************************************************************
  // ** TERMINAL
  // **************************************************************************
  "terminal.external.windowsExec": "wt.exe",
  // "terminal.integrated.fontFamily": "Source Code Pro",
  "terminal.integrated.fontFamily": "JetBrainsMono NF",
  "terminal.integrated.fontSize": 13,
  "terminal.integrated.fontWeight": "400",
  "terminal.integrated.showExitAlert": false,
  "terminal.integrated.splitCwd": "workspaceRoot",
  "terminal.integrated.profiles.windows": {
    "PowerShell": {
      "source": "PowerShell",
      "icon": "terminal-powershell"
    },
    "Command Prompt": {
      "path": [
        "${env:windir}\\Sysnative\\cmd.exe",
        "${env:windir}\\System32\\cmd.exe"
      ],
      "args": [],
      "icon": "terminal-cmd"
    },
    "Git Bash": null
  },
  "terminal.integrated.profiles.linux": {
    "bash": {
      "path": "bash",
      "icon": "terminal-bash"
    },
    "zsh": {
      "path": "zsh",
      "icon": "terminal-linux"
    },
    "tmux": {
      "path": "tmux",
      "icon": "terminal-tmux"
    }
  },
  "terminal.integrated.defaultProfile.linux": "zsh",
  "terminal.integrated.defaultProfile.windows": "PowerShell",
  "powershell.promptToUpdatePowerShell": false,
  // **************************************************************************
  // ** WORKBENCH
  // **************************************************************************
  "git.mergeEditor": true,
  "git.useEditorAsCommitInput": true,
  "git.terminalGitEditor": true,
  "workbench.colorCustomizations": {
    "[KDark]": {
      "editorCursor.foreground": "#e5c179",
      "editorInlayHint.background": "#37373700",
      "editorInlayHint.foreground": "#8f8f8f",
      "editorRuler.foreground": "#5a5a5a20"
    },
    "editorBracketHighlight.foreground1": "#73c1b9",
    "editorBracketHighlight.foreground2": "#80d3dd",
    "editorBracketHighlight.foreground3": "#9086a4",
    "editorBracketHighlight.foreground4": "#bfafc4",
    "editorBracketHighlight.foreground5": "#dec7d0",
    "editorBracketHighlight.foreground6": "#e09696",
    "editorBracketHighlight.unexpectedBracket.foreground": "#ffdca8"
  },
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "workbench.iconTheme": "nonicons",
  "workbench.settings.editor": "json",
  "workbench.startupEditor": "newUntitledFile",
  "workbench.tips.enabled": false,
  "workbench.productIconTheme": "icons-carbon",
  "security.workspace.trust.untrustedFiles": "open",
  "telemetry.telemetryLevel": "off",
  // **************************************************************************
  // ** WINDOWS
  // **************************************************************************
  "window.commandCenter": true,
  "window.title": "",
  "window.menuBarVisibility": "compact",
  "workbench.colorTheme": "KDark"
}